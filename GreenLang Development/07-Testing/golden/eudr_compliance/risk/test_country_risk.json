  "description": "Golden tests for country deforestation risk assessment under EUDR",
  "tests": [
    {
      "test_id": "RISK-HIGH-001",
      "description": "Brazil - Soya (HIGH risk)",
      "tool_name": "assess_country_risk",
      "input": {
        "country_code": "BR",
        "commodity_type": "soya",
        "region": null,
        "production_year": 2024
      },
      "expected_output": {
        "risk_level": "high",
        "country_name": "Brazil",
        "country_code": "BR",
        "due_diligence_level": "full_verification",
        "satellite_verification_required": true
      },
      "tolerance": 0.01,
      "tags": ["brazil", "soya", "high_risk"]
    },
