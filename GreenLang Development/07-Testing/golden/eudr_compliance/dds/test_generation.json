{
  "suite": "EUDR Due Diligence Statement Generation",
  "version": "1.0.0",
  "description": "Golden tests for DDS generation and compliance validation under EUDR",
  "tests": [
    {
      "test_id": "DDS-COMP-001",
      "description": "Compliant DDS - Low risk country, valid geolocation",
      "tool_name": "generate_dds_report",
      "input": {
        "operator_info": {
          "operator_id": "OP-DE-001",
          "company_name": "German Foods GmbH",
          "eori_number": "DE123456789000",
          "country_code": "DE",
          "contact_email": "compliance@germanfoods.de"
        },
        "commodity_data": {
          "cn_code": "09011100",
          "cn_description": "Green coffee beans",
          "commodity_type": "coffee",
          "quantity_kg": 25000.0,
          "production_country": "CO",
          "production_date": "2024-06-15"
        },
        "geolocation_data": {
          "coordinates": [4.5, -74.0],
          "coordinate_type": "point",
          "precision_meters": 10.0,
          "valid": true,
          "in_protected_area": false
        },
        "risk_assessment": {
          "risk_level": "low",
          "risk_score": 25,
          "due_diligence_level": "standard",
          "satellite_verification_required": false
        },
        "traceability_data": {
          "traceability_score": 95.0,
          "chain_of_custody": "complete",
          "origin_verified": true
        }
      },
      "expected_output": {
        "dds_status": "valid",
        "compliance_status": "compliant",
        "submission_ready": true
      },
      "tolerance": 0.01,
      "tags": ["compliant", "coffee", "low_risk"]
    },
