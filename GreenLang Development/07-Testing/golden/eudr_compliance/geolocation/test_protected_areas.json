{
  "suite": "EUDR Geolocation Validation - Protected Areas",
  "version": "1.0.0",
  "description": "Golden tests for protected area detection in EUDR geolocation validation",
  "tests": [
    {
      "test_id": "GEO-PA-001",
      "description": "Brazil Amazon Protected Complex - inside protected area",
      "tool_name": "validate_geolocation",
      "input": {
        "coordinates": [-3.0, -62.0],
        "country_code": "BR",
        "coordinate_type": "point",
        "precision_meters": 10.0
      },
      "expected_output": {
        "valid": true,
        "country_code": "BR",
        "in_protected_area": true,
        "protected_area_name": "Amazon Protected Complex"
      },
      "tolerance": 0.01,
      "tags": ["brazil", "amazon", "protected_area"]
    },
    {
