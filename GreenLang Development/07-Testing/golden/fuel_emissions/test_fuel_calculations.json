{
  "suite": "Fuel Emissions Calculations",
  "version": "1.0.0",
  "description": "Golden tests for fuel-based emission calculations with tolerance verification",
  "tests": [
    {
      "test_id": "FUEL-ELEC-001",
      "description": "Electricity - India grid factor",
      "tool_name": "calculate_fuel_emissions",
      "input": {
        "fuel_type": "electricity",
        "consumption": {"value": 1000000, "unit": "kWh"},
        "country": "IN"
      },
      "expected_output": {
        "co2e_emissions_kg": 710000.0,
        "emission_factor": 0.71,
        "unit": "kgCO2e"
      },
      "tolerance": 0.01,
      "tags": ["electricity", "india", "grid"]
    },
