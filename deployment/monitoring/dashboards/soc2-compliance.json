{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "enable": true,
        "expr": "ALERTS{alertname=~\"SOC2.*\", alertstate=\"firing\"}",
        "iconColor": "red",
        "name": "SOC 2 Alerts",
        "step": "60s"
      }
    ]
  },
  "description": "GreenLang SOC 2 Type II Compliance Dashboard - Readiness, Evidence, Testing, Findings, Attestations. PRD: SEC-009",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["greenlang", "compliance"],
      "targetBlank": true,
      "title": "Compliance Dashboards",
      "type": "dashboards"
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Overall SOC 2 Type II readiness score",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 60 },
              { "color": "yellow", "value": 80 },
              { "color": "green", "value": 90 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_readiness_percentage{category=\"overall\"}",
          "legendFormat": "Readiness",
          "refId": "A"
        }
      ],
      "title": "Readiness Score",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Evidence collection progress",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 6, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(increase(soc2_evidence_uploaded_total[30d]))",
          "legendFormat": "Evidence Collected",
          "refId": "A"
        }
      ],
      "title": "Evidence Items",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Control test pass rate",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 80 },
              { "color": "green", "value": 95 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 9, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "(sum(soc2_criteria_assessed_total{score_level=\"compliant\"}) / sum(soc2_criteria_assessed_total)) * 100",
          "legendFormat": "Pass Rate",
          "refId": "A"
        }
      ],
      "title": "Test Pass Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Total open audit findings",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 12, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(soc2_findings_total{status=\"open\"})",
          "legendFormat": "Open Findings",
          "refId": "A"
        }
      ],
      "title": "Open Findings",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Pending auditor requests",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 3 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 15, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(soc2_requests_total{status=\"open\"})",
          "legendFormat": "Pending Requests",
          "refId": "A"
        }
      ],
      "title": "Pending Requests",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Pending attestation signatures",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 18, "y": 1 },
      "id": 7,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_attestations_pending",
          "legendFormat": "Pending",
          "refId": "A"
        }
      ],
      "title": "Pending Attestations",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Project completion percentage",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 21, "y": 1 },
      "id": 8,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_project_completion_percentage{project_id=~\".*\"}",
          "legendFormat": "Completion",
          "refId": "A"
        }
      ],
      "title": "Project Progress",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 9,
      "title": "Categories",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Readiness score by trust service category",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Readiness %",
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineWidth": 1,
            "scaleDistribution": { "type": "linear" },
            "thresholdsStyle": { "mode": "line+area" }
          },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "transparent", "value": 80 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 10,
      "options": {
        "barRadius": 0.1,
        "barWidth": 0.6,
        "groupWidth": 0.7,
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom" },
        "orientation": "horizontal",
        "showValue": "always",
        "stacking": "none",
        "tooltip": { "mode": "single" },
        "xTickLabelRotation": 0,
        "xTickLabelSpacing": 0
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_readiness_percentage{category!=\"overall\"}",
          "legendFormat": "{{ category }}",
          "refId": "A"
        }
      ],
      "title": "Category Readiness",
      "type": "barchart"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Criteria coverage by category",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Compliant" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-background-solid" },
              { "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Partial" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-background-solid" },
              { "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Gap" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-background-solid" },
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 11,
      "options": {
        "footer": { "fields": "", "reducer": ["sum"], "show": true },
        "showHeader": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_compliant_criteria_total",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Criteria Coverage",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true }
          }
        }
      ],
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 12,
      "title": "Findings",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Open findings by severity level",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "none",
            "lineWidth": 1
          },
          "mappings": [],
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "critical" },
            "properties": [{ "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "high" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "medium" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "low" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 19 },
      "id": 13,
      "options": {
        "barRadius": 0.1,
        "barWidth": 0.6,
        "groupWidth": 0.7,
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom" },
        "orientation": "vertical",
        "showValue": "always",
        "stacking": "none",
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_gaps_total{risk_level=~\"critical|high|medium|low\"}",
          "legendFormat": "{{ risk_level }}",
          "refId": "A"
        }
      ],
      "title": "Open by Severity",
      "type": "barchart"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Finding count trend over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "stacking": { "mode": "none" }
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "critical" },
            "properties": [{ "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "high" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 19 },
      "id": 14,
      "options": {
        "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_gaps_total{category=\"all\"}",
          "legendFormat": "{{ risk_level }}",
          "refId": "A"
        }
      ],
      "title": "Finding Trend",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Remediation effort by severity",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "align": "auto", "displayMode": "auto" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 40 },
              { "color": "red", "value": 100 }
            ]
          },
          "unit": "h"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Hours" },
            "properties": [{ "id": "custom.displayMode", "value": "color-background" }]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 19 },
      "id": 15,
      "options": {
        "footer": { "fields": "", "reducer": ["sum"], "show": true },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Hours" }]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_gap_remediation_hours",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Remediation Effort",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true },
            "renameByName": { "Value": "Hours", "risk_level": "Severity" }
          }
        }
      ],
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 25 },
      "id": 16,
      "title": "Auditor Requests",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "SLA compliance rate",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 90 },
              { "color": "green", "value": 95 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 6, "w": 4, "x": 0, "y": 26 },
      "id": 17,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "100 - (sum(soc2_requests_overdue) / sum(soc2_requests_total) * 100)",
          "legendFormat": "SLA %",
          "refId": "A"
        }
      ],
      "title": "SLA Compliance",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Response time distribution by priority",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineWidth": 1
          },
          "mappings": [],
          "unit": "h"
        }
      },
      "gridPos": { "h": 6, "w": 10, "x": 4, "y": 26 },
      "id": 18,
      "options": {
        "bucketOffset": 0,
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "right" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.50, rate(soc2_request_sla_seconds_bucket[7d])) / 3600",
          "legendFormat": "{{ priority }} P50",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.95, rate(soc2_request_sla_seconds_bucket[7d])) / 3600",
          "legendFormat": "{{ priority }} P95",
          "refId": "B"
        }
      ],
      "title": "Response Time (hours)",
      "type": "histogram"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Overdue requests by priority",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "align": "auto", "displayMode": "auto" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Count" },
            "properties": [{ "id": "custom.displayMode", "value": "color-background" }]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 10, "x": 14, "y": 26 },
      "id": 19,
      "options": {
        "footer": { "fields": "", "reducer": ["sum"], "show": true },
        "showHeader": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "soc2_requests_overdue",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Overdue Requests",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true },
            "renameByName": { "Value": "Count", "priority": "Priority" }
          }
        }
      ],
      "type": "table"
    },
