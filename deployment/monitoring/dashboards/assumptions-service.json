      "targets": [
        { "expr": "sum(rate(gl_assumptions_versions_created_total[5m]))", "legendFormat": "Version Create Rate", "refId": "A" },
        { "expr": "sum(rate(gl_assumptions_versions_created_total[5m])) by (change_type)", "legendFormat": "{{change_type}}", "refId": "B" }
