          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 30 },
      "id": 12,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "gl_assumptions_by_category", "legendFormat": "{{category}}", "refId": "A" }
