{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Orchestrator Alerts",
        "expr": "ALERTS{alertname=~\"Orchestrator.*\", alertstate=\"firing\"}",
        "titleFormat": "Alert: {{alertname}}",
        "tagKeys": "severity"
      }
    ]
  },
  "description": "GreenLang Orchestrator Service - DAG Execution Engine monitoring with execution, node, parallel, checkpoint, provenance, and error panels",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["greenlang", "orchestrator"],
      "targetBlank": true,
      "title": "Orchestrator Dashboards",
      "type": "dashboards"
    },
    {
      "asDropdown": false,
      "icon": "dashboard",
      "includeVars": true,
      "keepTime": true,
      "title": "SLO Overview",
      "url": "/d/slo-overview",
      "type": "link"
    }
  ],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "panels": [],
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Number of DAG executions currently in running state",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30 },
              { "color": "red", "value": 45 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "gl_orchestrator_active_executions",
          "legendFormat": "Active Executions",
          "refId": "A"
        }
      ],
      "title": "Active Executions",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of DAG executions per second over the last 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(gl_orchestrator_dag_executions_total[5m]))",
          "legendFormat": "Execution Rate",
          "refId": "A"
        }
      ],
      "title": "Execution Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Percentage of DAG executions completing successfully over the last 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 80 },
              { "color": "yellow", "value": 90 },
              { "color": "green", "value": 95 }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(gl_orchestrator_dag_executions_total{status=\"completed\"}[5m])) / sum(rate(gl_orchestrator_dag_executions_total[5m])) * 100",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "title": "Success Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Average DAG execution duration over the last 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 300 }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(gl_orchestrator_dag_execution_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50 Duration",
          "refId": "A"
        }
      ],
      "title": "Avg Execution Duration",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "id": 101,
      "panels": [],
      "title": "DAG Execution",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "DAG execution rate by status over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 20, "showPoints": "never", "stacking": { "mode": "normal" } },
          "unit": "ops"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "failed" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "completed" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "cancelled" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 7 },
      "id": 5,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_orchestrator_dag_executions_total[5m])) by (status)", "legendFormat": "{{status}}", "refId": "A" }
      ],
      "title": "Executions by Status",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of DAG execution durations",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10, "showPoints": "never" },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 7 },
      "id": 6,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(gl_orchestrator_dag_execution_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.90, sum(rate(gl_orchestrator_dag_execution_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p90", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(gl_orchestrator_dag_execution_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p99", "refId": "C" }
      ],
      "title": "Execution Duration Percentiles",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Top 10 slowest DAG workflows by average execution duration",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 14 },
      "id": 7,
      "options": { "orientation": "horizontal", "barWidth": 0.7, "showValue": "always" },
      "targets": [
        { "expr": "topk(10, histogram_quantile(0.95, sum(rate(gl_orchestrator_dag_execution_duration_seconds_bucket[1h])) by (dag_id, le)))", "legendFormat": "{{dag_id}}", "refId": "A", "format": "table", "instant": true }
      ],
      "title": "Top 10 Slowest DAGs (p95)",
      "type": "barchart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 21 },
      "id": 102,
      "panels": [],
      "title": "Node Execution",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Node execution rate by status",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never", "stacking": { "mode": "normal" } },
          "unit": "ops"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "failed" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "completed" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "timed_out" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "skipped" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 22 },
      "id": 8,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_orchestrator_node_executions_total[5m])) by (status)", "legendFormat": "{{status}}", "refId": "A" }
      ],
      "title": "Node Executions by Status",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Heatmap of node execution durations showing distribution over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "scheme", "schemeName": "Oranges" },
          "custom": { "hideFrom": { "legend": false, "tooltip": false, "viz": false } }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 22 },
      "id": 9,
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": { "scheme": "Oranges", "fill": "dark-orange" },
        "yAxis": { "unit": "s" }
      },
      "targets": [
        { "expr": "sum(increase(gl_orchestrator_node_execution_duration_seconds_bucket[5m])) by (le)", "legendFormat": "{{le}}", "refId": "A", "format": "heatmap" }
      ],
      "title": "Node Duration Heatmap",
      "type": "heatmap"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Node retry rate and timeout rate",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 29 },
      "id": 10,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_orchestrator_node_retries_total[5m]))", "legendFormat": "Retries/s", "refId": "A" },
        { "expr": "sum(rate(gl_orchestrator_node_timeouts_total[5m]))", "legendFormat": "Timeouts/s", "refId": "B" }
      ],
      "title": "Node Retries & Timeouts Rate",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 },
      "id": 103,
      "panels": [],
      "title": "Parallel Execution",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of parallel nodes per execution level",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never" },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 37 },
      "id": 11,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(gl_orchestrator_parallel_nodes_per_level_bucket[5m])) by (le))", "legendFormat": "p50 nodes/level", "refId": "A" },
        { "expr": "histogram_quantile(0.90, sum(rate(gl_orchestrator_parallel_nodes_per_level_bucket[5m])) by (le))", "legendFormat": "p90 nodes/level", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(gl_orchestrator_parallel_nodes_per_level_bucket[5m])) by (le))", "legendFormat": "p99 nodes/level", "refId": "C" }
      ],
      "title": "Parallel Nodes per Level",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Concurrency utilization: active executions vs maximum allowed",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.7 },
              { "color": "red", "value": 0.9 }
            ]
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 37 },
      "id": 12,
      "options": {
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        { "expr": "gl_orchestrator_active_executions / 50", "legendFormat": "Concurrency Utilization", "refId": "A" }
      ],
      "title": "Concurrency Utilization",
      "type": "gauge"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 44 },
      "id": 104,
      "panels": [],
      "title": "Checkpoints",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Checkpoint operation rate by type (save, load, verify, cleanup)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 45 },
      "id": 13,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_orchestrator_checkpoint_operations_total[5m])) by (operation)", "legendFormat": "{{operation}}", "refId": "A" }
      ],
      "title": "Checkpoint Operations",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of checkpoint sizes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10, "showPoints": "never" },
          "unit": "bytes"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 45 },
      "id": 14,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(gl_orchestrator_checkpoint_size_bytes_bucket[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.90, sum(rate(gl_orchestrator_checkpoint_size_bytes_bucket[5m])) by (le))", "legendFormat": "p90", "refId": "B" },
        { "expr": "histogram_quantile(0.95, sum(rate(gl_orchestrator_checkpoint_size_bytes_bucket[5m])) by (le))", "legendFormat": "p95", "refId": "C" }
      ],
      "title": "Checkpoint Size Distribution",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 52 },
      "id": 105,
      "panels": [],
      "title": "Provenance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of provenance chain lengths (number of nodes in provenance chain)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never" },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 53 },
      "id": 15,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(gl_orchestrator_provenance_chain_length_bucket[5m])) by (le))", "legendFormat": "p50 chain length", "refId": "A" },
        { "expr": "histogram_quantile(0.90, sum(rate(gl_orchestrator_provenance_chain_length_bucket[5m])) by (le))", "legendFormat": "p90 chain length", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(gl_orchestrator_provenance_chain_length_bucket[5m])) by (le))", "legendFormat": "p99 chain length", "refId": "C" }
      ],
      "title": "Provenance Chain Length",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Provenance chain verification success rate",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 100 }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 53 },
      "id": 16,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "100",
          "legendFormat": "Verification Rate (target: 100%)",
          "refId": "A"
        }
      ],
      "title": "Chain Verification Rate",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 60 },
      "id": 106,
      "panels": [],
      "title": "Errors",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "DAG validation error rate by error type",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80, "showPoints": "never", "stacking": { "mode": "normal" } },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 61 },
      "id": 17,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_orchestrator_dag_validation_errors_total[5m])) by (error_type)", "legendFormat": "{{error_type}}", "refId": "A" }
      ],
      "title": "Validation Errors by Type",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "DAG execution failure rate over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 20, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 61 },
      "id": 18,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
