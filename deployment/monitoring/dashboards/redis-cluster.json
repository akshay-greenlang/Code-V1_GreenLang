{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Redis Cluster Monitoring Dashboard - INFRA-003",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Redis Cluster Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "title": "Connected Clients",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(redis_connected_clients{job=\"redis\"})",
          "legendFormat": "Clients"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 900 }
            ]
          }
        }
      }
    },
    {
      "title": "Memory Usage",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "redis_memory_used_bytes{job=\"redis\"} / redis_memory_max_bytes{job=\"redis\"} * 100",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      }
    },
    {
      "title": "Commands/sec",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(redis_commands_processed_total{job=\"redis\"}[5m]))",
          "legendFormat": "ops/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(redis_keyspace_hits_total{job=\"redis\"}[5m])) / (sum(rate(redis_keyspace_hits_total{job=\"redis\"}[5m])) + sum(rate(redis_keyspace_misses_total{job=\"redis\"}[5m]))) * 100",
          "legendFormat": "Hit Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 80 },
              { "color": "green", "value": 95 }
            ]
          }
        }
      }
    },
    {
      "title": "Evictions/sec",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(redis_evicted_keys_total{job=\"redis\"}[5m]))",
          "legendFormat": "Evictions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      }
    },
    {
      "title": "Replication Lag (bytes)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "max(redis_replication_backlog_bytes{job=\"redis\"})",
          "legendFormat": "Backlog"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10485760 },
              { "color": "red", "value": 104857600 }
            ]
          }
        }
      }
    },
    {
      "title": "Performance",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }
    },
    {
      "title": "Command Latency (P50/P95/P99)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(redis_commands_duration_seconds_bucket{job=\"redis\"}[5m])) by (le))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(redis_commands_duration_seconds_bucket{job=\"redis\"}[5m])) by (le))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(redis_commands_duration_seconds_bucket{job=\"redis\"}[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Throughput by Command Type",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "topk(10, sum by (cmd) (rate(redis_commands_total{job=\"redis\"}[5m])))",
          "legendFormat": "{{cmd}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "title": "Memory",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }
    },
    {
      "title": "Memory Usage Over Time",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "targets": [
        {
          "expr": "redis_memory_used_bytes{job=\"redis\"}",
          "legendFormat": "Used - {{instance}}"
        },
        {
          "expr": "redis_memory_max_bytes{job=\"redis\"}",
          "legendFormat": "Max - {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "title": "Key Count by Database",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "targets": [
        {
          "expr": "sum by (db) (redis_db_keys{job=\"redis\"})",
          "legendFormat": "{{db}}"
        }
      ]
    },
    {
      "title": "Replication",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }
    },
    {
      "title": "Replication Offset Delta",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "redis_master_repl_offset{job=\"redis\"} - on(instance) redis_slave_repl_offset{job=\"redis\"}",
          "legendFormat": "Offset Delta - {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "title": "Connected Replicas",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "redis_connected_slaves{job=\"redis\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "title": "Connections",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 }
    },
    {
      "title": "Connection Rate",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 },
      "targets": [
        {
          "expr": "rate(redis_connections_received_total{job=\"redis\"}[5m])",
          "legendFormat": "New conn/s - {{instance}}"
        },
        {
          "expr": "rate(redis_rejected_connections_total{job=\"redis\"}[5m])",
          "legendFormat": "Rejected/s - {{instance}}"
        }
      ]
    },
    {
      "title": "Blocked Clients",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 },
      "targets": [
        {
          "expr": "redis_blocked_clients{job=\"redis\"}",
          "legendFormat": "Blocked - {{instance}}"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["redis", "infra-003", "greenlang"],
  "templating": {
    "list": [
      {
        "current": { "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "label": "Datasource",
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "utc",
