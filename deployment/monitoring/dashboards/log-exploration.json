    {
      "asDropdown": true,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["greenlang", "logs"],
      "targetBlank": false,
      "title": "Log Dashboards",
      "tooltip": "Related Log Dashboards",
      "type": "dashboards"
