                  }
                ]
              },
              { "id": "custom.width", "value": 300 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "timestamp" },
            "properties": [
              { "id": "custom.width", "value": 200 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "app" },
            "properties": [
              { "id": "custom.width", "value": 150 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "level" },
            "properties": [
              { "id": "custom.width", "value": 80 }
            ]
          }
