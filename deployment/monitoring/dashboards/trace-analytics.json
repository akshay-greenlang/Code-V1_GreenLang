  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["tracing", "analytics", "observability", "obs-003", "greenlang", "tempo"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Thanos", "value": "Thanos" },
        "hide": 0,
        "includeAll": false,
