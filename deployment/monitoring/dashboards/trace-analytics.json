        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      },
      {
        "current": { "selected": false, "text": "Tempo", "value": "Tempo" },
        "hide": 0,
        "includeAll": false,
        "label": "Tempo Datasource",
        "multi": false,
        "name": "tempo_datasource",
        "options": [],
        "query": "tempo",
        "refresh": 1,
