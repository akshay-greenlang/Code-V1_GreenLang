      "targets": [
        { "expr": "gl_normalizer_vocabulary_entries", "legendFormat": "{{vocabulary_type}}", "refId": "A" },
        { "expr": "gl_normalizer_vocabulary_cache_age_seconds", "legendFormat": "Cache Age ({{vocabulary_type}})", "refId": "B" }
      ],
      "title": "Vocabulary Cache",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 53 },
      "id": 106,
      "panels": [],
      "title": "Infrastructure",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Normalizer service up/down status",
