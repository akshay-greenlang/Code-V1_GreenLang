        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Normalizer Alerts",
        "expr": "ALERTS{alertname=~\"Normalizer.*\", alertstate=\"firing\"}",
        "titleFormat": "Alert: {{alertname}}",
        "tagKeys": "severity"
      }
    ]
  },
  "description": "GreenLang Unit & Reference Normalizer Service - conversion, GHG/GWP, entity resolution, dimensional analysis, cache, and infrastructure monitoring",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["greenlang", "normalizer"],
      "targetBlank": true,
      "title": "Normalizer Dashboards",
      "type": "dashboards"
    },
    {
      "asDropdown": false,
      "icon": "dashboard",
      "includeVars": true,
      "keepTime": true,
      "title": "SLO Overview",
      "url": "/d/slo-overview",
      "type": "link"
    }
  ],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "panels": [],
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of unit conversion requests per second over the last 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(gl_normalizer_conversions_total[5m]))",
          "legendFormat": "Conversion Rate",
          "refId": "A"
        }
      ],
      "title": "Conversion Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of entity resolution requests per second over the last 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
            ]
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(gl_normalizer_resolutions_total[5m]))",
          "legendFormat": "Resolution Rate",
          "refId": "A"
        }
      ],
      "title": "Resolution Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of conversion errors per second over the last 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(gl_normalizer_conversions_total{status=\"error\"}[5m]))",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Number of conversions currently in progress",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "gl_normalizer_active_conversions",
          "legendFormat": "Active Conversions",
          "refId": "A"
        }
      ],
      "title": "Active Conversions",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "id": 101,
      "panels": [],
      "title": "Conversion Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Heatmap of conversion request durations showing distribution over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "scheme", "schemeName": "Oranges" },
          "custom": { "hideFrom": { "legend": false, "tooltip": false, "viz": false } }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 7 },
      "id": 5,
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": { "scheme": "Oranges", "fill": "dark-orange" },
        "yAxis": { "unit": "s" }
      },
      "targets": [
        { "expr": "sum(increase(gl_normalizer_conversion_duration_seconds_bucket[5m])) by (le)", "legendFormat": "{{le}}", "refId": "A", "format": "heatmap" }
      ],
      "title": "Conversion Latency Histogram",
      "type": "heatmap"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Conversion duration percentiles (p50, p95, p99) over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10, "showPoints": "never" },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 7 },
      "id": 6,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(gl_normalizer_conversion_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(gl_normalizer_conversion_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(gl_normalizer_conversion_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p99", "refId": "C" }
      ],
      "title": "Conversion Latency Percentiles (p50/p95/p99)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Conversion rate broken down by physical dimension over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never", "stacking": { "mode": "normal" } },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 14 },
      "id": 7,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_conversions_total[5m])) by (dimension)", "legendFormat": "{{dimension}}", "refId": "A" }
      ],
      "title": "Conversion Rate by Dimension",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 21 },
      "id": 102,
      "panels": [],
      "title": "GHG / GWP Conversions",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "GWP conversion rate broken down by greenhouse gas",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "CO2" }, "properties": [{ "id": "color", "value": { "fixedColor": "semi-dark-blue", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "CH4" }, "properties": [{ "id": "color", "value": { "fixedColor": "semi-dark-orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "N2O" }, "properties": [{ "id": "color", "value": { "fixedColor": "semi-dark-green", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 22 },
      "id": 8,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_gwp_conversions_total[5m])) by (gas)", "legendFormat": "{{gas}}", "refId": "A" }
      ],
      "title": "GWP Conversions by Gas",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of emission unit types used in conversions",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80, "showPoints": "never", "stacking": { "mode": "normal" } },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 22 },
      "id": 9,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_conversions_total{dimension=~\"emission.*\"}[5m])) by (from_unit)", "legendFormat": "{{from_unit}}", "refId": "A" }
      ],
      "title": "Emission Unit Distribution",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 29 },
      "id": 103,
      "panels": [],
      "title": "Entity Resolution",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Entity resolution rate broken down by entity type (fuel, material, process, gas)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 30 },
      "id": 10,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_resolutions_total[5m])) by (entity_type)", "legendFormat": "{{entity_type}}", "refId": "A" }
      ],
      "title": "Resolution Rate by Type",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of entity resolution confidence scores",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 30 },
      "id": 11,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_resolutions_total[5m])) by (confidence_level)", "legendFormat": "{{confidence_level}}", "refId": "A" }
      ],
      "title": "Confidence Distribution",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Percentage of entity resolution attempts that failed to resolve",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 30 },
      "id": 12,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "(sum(rate(gl_normalizer_resolutions_total{resolved=\"false\"}[5m])) / sum(rate(gl_normalizer_resolutions_total[5m]))) * 100",
          "legendFormat": "Unresolved %",
          "refId": "A"
        }
      ],
      "title": "Unresolved Rate",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 37 },
      "id": 104,
      "panels": [],
      "title": "Dimensional Analysis",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of dimension mismatch errors in conversion requests",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Dimension Errors" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 38 },
      "id": 13,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_dimension_errors_total[5m]))", "legendFormat": "Dimension Errors", "refId": "A" },
        { "expr": "sum(rate(gl_normalizer_dimension_errors_total[5m])) by (from_dimension, to_dimension)", "legendFormat": "{{from_dimension}} -> {{to_dimension}}", "refId": "B" }
      ],
      "title": "Dimension Mismatch Errors",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Cross-dimension conversion attempts that required dimensional analysis",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 60, "showPoints": "never", "stacking": { "mode": "normal" } },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 38 },
      "id": 14,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_cross_dimension_total[5m])) by (dimension_pair)", "legendFormat": "{{dimension_pair}}", "refId": "A" }
      ],
      "title": "Cross-Dimension Attempts",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 45 },
      "id": 105,
      "panels": [],
      "title": "Cache Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Unit conversion cache hit and miss rates over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "showPoints": "never" },
          "unit": "ops"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Hits/s" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Misses/s" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 46 },
      "id": 15,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(gl_normalizer_cache_hits_total[5m]))", "legendFormat": "Hits/s", "refId": "A" },
        { "expr": "sum(rate(gl_normalizer_cache_misses_total[5m]))", "legendFormat": "Misses/s", "refId": "B" }
      ],
      "title": "Cache Hit/Miss Rates",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Cache hit ratio - percentage of conversions served from cache",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.5 },
              { "color": "yellow", "value": 0.7 },
              { "color": "green", "value": 0.8 }
            ]
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 46 },
      "id": 16,
      "options": {
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(rate(gl_normalizer_cache_hits_total[5m])) / (sum(rate(gl_normalizer_cache_hits_total[5m])) + sum(rate(gl_normalizer_cache_misses_total[5m])))",
          "legendFormat": "Cache Hit Ratio",
          "refId": "A"
        }
      ],
      "title": "Cache Hit Ratio",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Vocabulary cache status showing loaded vocabulary datasets and their age",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
