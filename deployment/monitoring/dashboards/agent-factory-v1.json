{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "enable": true,
        "expr": "ALERTS{alertname=~\"AgentFactory.*\", alertstate=\"firing\"}",
        "iconColor": "red",
        "name": "Agent Factory Alerts",
        "step": "60s"
      }
    ]
  },
  "description": "GreenLang Agent Factory v1.0 - Lifecycle, Queue, Resilience, Cost Monitoring. PRD: INFRA-010",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["greenlang"],
      "targetBlank": true,
      "title": "GreenLang Dashboards",
      "type": "dashboards"
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Total number of registered agents by current lifecycle status",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "running" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "degraded" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "failed" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 4, "w": 5, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (status) (agent_factory_agents_registered{namespace=~\"$namespace\"})",
          "legendFormat": "{{ status }}",
          "refId": "A"
        }
      ],
      "title": "Agent Count by Status",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Total agent executions per hour",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 100 },
              { "color": "yellow", "value": 5000 },
              { "color": "red", "value": 10000 }
            ]
          },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 4, "w": 5, "x": 5, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(agent_factory_executions_total{namespace=~\"$namespace\"}[5m])) * 3600",
          "legendFormat": "executions/hr",
          "refId": "A"
        }
      ],
      "title": "Total Executions/Hour",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Average execution duration across all agents",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 30 }
            ]
          },
          "unit": "s"
        }
      },
      "gridPos": { "h": 4, "w": 5, "x": 10, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(agent_factory_execution_duration_seconds_bucket{namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        }
      ],
      "title": "Avg Execution Duration",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Percentage of failed executions in the last 5 minutes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 4, "w": 5, "x": 15, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "(sum(rate(agent_factory_execution_errors_total{namespace=~\"$namespace\"}[5m])) / sum(rate(agent_factory_executions_total{namespace=~\"$namespace\"}[5m]))) * 100",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Current queue depth across all priority levels",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "orange", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(agent_factory_queue_depth{namespace=~\"$namespace\"})",
          "legendFormat": "Queue Depth",
          "refId": "A"
        }
      ],
      "title": "Queue Depth",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 7,
      "title": "Lifecycle",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Agent state transitions over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "transitions/min",
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" }
          },
          "unit": "opm"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "id": 8,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (from_state, to_state) (rate(agent_factory_state_transitions_total{namespace=~\"$namespace\"}[5m])) * 60",
          "legendFormat": "{{ from_state }} -> {{ to_state }}",
          "refId": "A"
        }
      ],
      "title": "Agent State Transitions",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Health check results over time (success vs failure)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisLabel": "checks/min",
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "opm"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "failures" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "successes" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 6 },
      "id": 9,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(agent_factory_health_check_total{namespace=~\"$namespace\", result=\"success\"}[5m])) * 60",
          "legendFormat": "successes",
          "refId": "A"
        },
        {
          "expr": "sum(rate(agent_factory_health_check_failures_total{namespace=~\"$namespace\"}[5m])) * 60",
          "legendFormat": "failures",
          "refId": "B"
        }
      ],
      "title": "Health Check Results",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of active agents by type (deterministic, reasoning, insight)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "hideFrom": { "legend": false, "tooltip": false, "viz": false } }
        }
      },
      "gridPos": { "h": 8, "w": 4, "x": 16, "y": 6 },
      "id": 10,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "pieType": "donut",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum by (agent_type) (agent_factory_agents_registered{namespace=~\"$namespace\", status=\"running\"})",
          "legendFormat": "{{ agent_type }}",
          "refId": "A"
        }
      ],
      "title": "Active Agents by Type",
      "type": "piechart"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "description": "Recent agent startup and shutdown events from Loki",
      "gridPos": { "h": 8, "w": 4, "x": 20, "y": 6 },
      "id": 11,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "{namespace=\"greenlang-agent-factory\"} |= \"state_transition\" | json | line_format \"{{.agent_key}}: {{.from_state}} -> {{.to_state}}\"",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Startup/Shutdown Events",
      "type": "logs"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 12,
      "title": "Task Queue",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Queue depth broken down by task priority level",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "tasks",
            "drawStyle": "line",
            "fillOpacity": 30,
            "lineWidth": 2,
            "stacking": { "group": "A", "mode": "normal" }
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "critical" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "high" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "normal" }, "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "low" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "background" }, "properties": [{ "id": "color", "value": { "fixedColor": "purple", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 15 },
      "id": 13,
      "options": { "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (priority) (agent_factory_queue_depth{namespace=~\"$namespace\"})",
          "legendFormat": "{{ priority }}",
          "refId": "A"
        }
      ],
      "title": "Queue Depth by Priority",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Task enqueue and dequeue rates",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "tasks/sec",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 15 },
      "id": 14,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(agent_factory_tasks_enqueued_total{namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "enqueued",
          "refId": "A"
        },
        {
          "expr": "sum(rate(agent_factory_tasks_dequeued_total{namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "dequeued",
          "refId": "B"
        },
        {
          "expr": "sum(rate(agent_factory_tasks_completed_total{namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "completed",
          "refId": "C"
        }
      ],
      "title": "Task Throughput",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Percentage of workers currently processing tasks",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "orange", "value": 80 },
              { "color": "red", "value": 95 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 15 },
      "id": 15,
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "(sum(agent_factory_workers_busy{namespace=~\"$namespace\"}) / sum(agent_factory_workers_total{namespace=~\"$namespace\"})) * 100",
          "legendFormat": "Worker Utilization",
          "refId": "A"
        }
      ],
      "title": "Worker Utilization",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Number of messages in the dead-letter queue",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "orange", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 15 },
      "id": 16,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(agent_factory_dlq_size{namespace=~\"$namespace\"})",
          "legendFormat": "DLQ Size",
          "refId": "A"
        }
      ],
      "title": "Dead-Letter Queue Size",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 17,
      "title": "Resilience",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Circuit breaker state for each agent over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "fillOpacity": 80, "lineWidth": 0 },
          "mappings": [
            { "options": { "0": { "color": "green", "text": "CLOSED" } }, "type": "value" },
            { "options": { "1": { "color": "red", "text": "OPEN" } }, "type": "value" },
            { "options": { "2": { "color": "yellow", "text": "HALF_OPEN" } }, "type": "value" }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 },
              { "color": "yellow", "value": 2 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 24 },
      "id": 18,
      "options": {
        "alignValue": "center",
        "mergeValues": true,
        "rowHeight": 0.8,
        "showValue": "auto",
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "agent_factory_circuit_breaker_state{namespace=~\"$namespace\", state=\"open\"} * 1 or agent_factory_circuit_breaker_state{namespace=~\"$namespace\", state=\"half_open\"} * 2 or agent_factory_circuit_breaker_state{namespace=~\"$namespace\", state=\"closed\"} * 0",
          "legendFormat": "{{ agent_key }}",
          "refId": "A"
        }
      ],
      "title": "Circuit Breaker States",
      "type": "state-timeline"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of fallback handler invocations per agent",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "invocations/min",
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          },
          "unit": "opm"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 24 },
      "id": 19,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (agent_key) (rate(agent_factory_fallback_invocations_total{namespace=~\"$namespace\"}[5m])) * 60",
          "legendFormat": "{{ agent_key }}",
          "refId": "A"
        }
      ],
      "title": "Fallback Invocations",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of retries per agent",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "retries/min",
            "drawStyle": "bars",
            "fillOpacity": 50,
            "lineWidth": 1,
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "opm"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 24 },
      "id": 20,
      "options": { "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (agent_key) (rate(agent_factory_retries_total{namespace=~\"$namespace\"}[5m])) * 60",
          "legendFormat": "{{ agent_key }}",
          "refId": "A"
        }
      ],
      "title": "Retry Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of execution timeouts per agent",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "timeouts/min",
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          },
          "unit": "opm"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 24 },
      "id": 21,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (agent_key) (rate(agent_factory_execution_timeouts_total{namespace=~\"$namespace\"}[5m])) * 60",
          "legendFormat": "{{ agent_key }}",
          "refId": "A"
        }
      ],
      "title": "Timeout Rate",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
