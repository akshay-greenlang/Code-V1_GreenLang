{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "enable": true,
        "expr": "ALERTS{alertname=~\"(HighLoginFailureRate|AccountLockoutSpike|TokenTheftDetected|BruteForceDetected|AuthServiceDown|AuthServiceHighLatency).*\", alertstate=\"firing\"}",
        "iconColor": "red",
        "name": "Auth Service Alerts",
        "step": "60s"
      }
    ]
  },
  "description": "GreenLang JWT Authentication Service - Login, Token Lifecycle, Security, Performance. PRD: SEC-001",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["greenlang"],
      "targetBlank": true,
      "title": "GreenLang Dashboards",
      "type": "dashboards"
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Login rate split by success and failure outcomes over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "logins/sec",
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "stacking": { "mode": "none" }
          },
          "unit": "ops"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "failure" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "legend": { "calcs": ["mean", "max", "lastNotNull"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(gl_auth_login_total{result=\"success\", namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "success",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(gl_auth_login_total{result=\"failure\", namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "failure",
          "refId": "B"
        }
      ],
      "title": "Login Rate (Success vs Failure)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Number of currently active authenticated sessions",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5000 },
              { "color": "red", "value": 10000 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(gl_auth_active_sessions{namespace=~\"$namespace\"})",
          "legendFormat": "Active Sessions",
          "refId": "A"
        }
      ],
      "title": "Active Sessions",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of access and refresh token issuance over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "tokens/sec",
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2,
            "stacking": { "mode": "none" }
          },
          "unit": "ops"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "access" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "refresh" },
            "properties": [{ "id": "color", "value": { "fixedColor": "purple", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 1 },
      "id": 4,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(gl_auth_token_issued_total{type=\"access\", namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "access",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(gl_auth_token_issued_total{type=\"refresh\", namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "refresh",
          "refId": "B"
        }
      ],
      "title": "Token Issuance Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Current authentication error rate as a percentage of total requests",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "orange", "value": 15 },
              { "color": "red", "value": 30 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "(sum(rate(gl_auth_login_total{result=\"failure\", namespace=~\"$namespace\"}[5m])) / sum(rate(gl_auth_login_total{namespace=~\"$namespace\"}[5m]))) * 100",
          "legendFormat": "Error Rate %",
          "refId": "A"
        }
      ],
      "title": "Auth Error Rate",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 6,
      "title": "Token Lifecycle",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Token validation latency at P50, P95, and P99 percentiles",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "latency",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 10 },
      "id": 7,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.50, sum(rate(gl_auth_token_validate_duration_seconds_bucket{namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.95, sum(rate(gl_auth_token_validate_duration_seconds_bucket{namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.99, sum(rate(gl_auth_token_validate_duration_seconds_bucket{namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "title": "Token Validation Latency (P50/P95/P99)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Token revocations categorized by reason (logout, theft, admin, expiry)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "revocations",
            "drawStyle": "bars",
            "fillOpacity": 80,
            "lineWidth": 1,
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "token_theft" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 10 },
      "id": 8,
      "options": {
        "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (reason) (increase(gl_auth_token_revoked_total{namespace=~\"$namespace\"}[1h]))",
          "legendFormat": "{{ reason }}",
          "refId": "A"
        }
      ],
      "title": "Token Revocations by Reason",
      "type": "barchart"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of refresh token rotations over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "rotations/sec",
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 10 },
      "id": 9,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(gl_auth_token_refresh_total{namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "Refresh Rotation Rate",
          "refId": "A"
        }
      ],
      "title": "Refresh Token Rotation Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Current number of JTIs in the token blacklist",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 1000000,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500000 },
              { "color": "orange", "value": 800000 },
              { "color": "red", "value": 950000 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 10 },
      "id": 10,
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(gl_auth_blacklist_size{namespace=~\"$namespace\"})",
          "legendFormat": "Blacklist Size",
          "refId": "A"
        }
      ],
      "title": "JTI Blacklist Size",
      "type": "gauge"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 11,
      "title": "Security",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Top source IPs with failed login attempts in the last hour",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          },
          "custom": {
            "align": "auto",
            "displayMode": "color-background-solid"
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 19 },
      "id": 12,
      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Value" }],
        "footer": { "show": false }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "topk(20, sum by (source_ip) (increase(gl_auth_login_total{result=\"failure\", namespace=~\"$namespace\"}[1h])))",
          "legendFormat": "{{ source_ip }}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "title": "Failed Login Attempts by IP (1h)",
      "type": "table"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Account lockout events over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "orange" },
          "custom": {
            "axisLabel": "lockouts",
            "drawStyle": "line",
            "fillOpacity": 30,
            "lineWidth": 2,
            "pointSize": 5
          },
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 19 },
      "id": 13,
      "options": {
        "legend": { "calcs": ["sum", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(increase(gl_auth_lockout_total{namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "Lockouts",
          "refId": "A"
        }
      ],
      "title": "Account Lockouts",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Permission denials grouped by the target endpoint",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "denials",
            "drawStyle": "bars",
            "fillOpacity": 80,
            "lineWidth": 1,
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 19 },
      "id": 14,
      "options": {
        "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (endpoint) (increase(gl_auth_permission_denied_total{namespace=~\"$namespace\"}[1h]))",
          "legendFormat": "{{ endpoint }}",
          "refId": "A"
        }
      ],
      "title": "Permission Denials by Endpoint",
      "type": "barchart"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "MFA verification success and failure rate over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "verifications/sec",
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          },
          "unit": "ops"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "failure" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 19 },
      "id": 15,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(gl_auth_mfa_verify_total{result=\"success\", namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "success",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(gl_auth_mfa_verify_total{result=\"failure\", namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "failure",
          "refId": "B"
        }
      ],
      "title": "MFA Verification Rate",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 16,
      "title": "Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Distribution of login latency as a histogram with P50, P95, P99 overlays",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "latency",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 2 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 28 },
      "id": 17,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.50, sum(rate(gl_auth_login_duration_seconds_bucket{namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.95, sum(rate(gl_auth_login_duration_seconds_bucket{namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.99, sum(rate(gl_auth_login_duration_seconds_bucket{namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "title": "Login Latency Distribution",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "CPU and memory usage of auth service pods",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 28 },
      "id": 18,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{namespace=\"greenlang-auth\", container=\"auth-service\"}[5m]))",
          "legendFormat": "CPU: {{ pod }}",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (pod) (container_memory_working_set_bytes{namespace=\"greenlang-auth\", container=\"auth-service\"}) / 1024 / 1024",
          "legendFormat": "Mem(Mi): {{ pod }}",
          "refId": "B"
        }
      ],
      "title": "Auth Service CPU/Memory",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Rate of requests rejected by the auth service rate limiter",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "rejections/sec",
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5
          },
          "unit": "ops",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "login" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "token_refresh" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
