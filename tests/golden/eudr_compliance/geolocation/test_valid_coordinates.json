{
  "suite": "EUDR Geolocation Validation - Valid Coordinates",
  "version": "1.0.0",
  "description": "Golden tests for valid GPS coordinate validation across EUDR-regulated countries",
  "tests": [
    {
      "test_id": "GEO-001",
      "description": "Brazil Amazon region - valid point coordinates",
      "tool_name": "validate_geolocation",
      "input": {
        "coordinates": [-3.1, -60.0],
        "country_code": "BR",
        "coordinate_type": "point",
        "precision_meters": 10.0
      },
      "expected_output": {
        "valid": true,
        "country_code": "BR",
        "in_protected_area": false
      },
      "tolerance": 0.01,
      "tags": ["brazil", "amazon", "point"]
    },
