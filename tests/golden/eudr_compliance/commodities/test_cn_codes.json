{
  "suite": "EUDR Commodity Classification - CN Codes",
  "version": "1.0.0",
  "description": "Golden tests for EU Combined Nomenclature code classification under EUDR",
  "tests": [
    {
      "test_id": "CN-CATTLE-001",
      "description": "Cattle - Live purebred breeding bovine animals",
      "tool_name": "classify_commodity",
      "input": {
        "cn_code": "01022110",
        "product_description": "Live purebred breeding bovine animals",
        "quantity_kg": 5000.0
      },
      "expected_output": {
        "eudr_regulated": true,
        "commodity_type": "cattle",
        "cn_code": "01022110",
        "risk_category": "high"
      },
      "tolerance": 0.01,
      "tags": ["cattle", "live_animals"]
    },
