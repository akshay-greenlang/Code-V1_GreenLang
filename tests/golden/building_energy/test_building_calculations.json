{
  "suite": "Building Energy Calculations",
  "version": "1.0.0",
  "description": "Golden tests for building energy performance and emissions calculations",
  "tests": [
    {
      "test_id": "BLDG-EUI-001",
      "description": "Office building EUI calculation - US",
      "tool_name": "calculate_building_energy",
      "input": {
        "building_type": "office",
        "gross_floor_area_sqm": 10000,
        "annual_electricity_kwh": 1500000,
        "annual_gas_therms": 15000,
        "country": "US",
        "climate_zone": "4A"
      },
      "expected_output": {
        "eui_kwh_per_sqm": 229.5,
        "total_emissions_tco2e": 656.25,
        "energy_star_eligible": true
      },
      "tolerance": 0.01,
      "tags": ["office", "eui", "usa"]
    },
