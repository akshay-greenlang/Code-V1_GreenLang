# {{ cookiecutter.agent_name }} Agent Configuration
# Auto-generated by GreenLang Agent Factory

id: {{ cookiecutter.agent_id }}
name: {{ cookiecutter.agent_name }}
version: {{ cookiecutter.version }}
type: {{ cookiecutter.agent_type }}
domain: {{ cookiecutter.domain }}
status: active

description: |
  {{ cookiecutter.description }}

metadata:
  created: {{ cookiecutter.created_at }}
  updated: {{ cookiecutter.created_at }}
  author: {{ cookiecutter.author }}
  license: {{ cookiecutter.license }}

capabilities:
{% for capability in cookiecutter.capabilities %}
  - {{ capability }}
{% endfor %}

architecture:
  pattern: {{ cookiecutter.architecture_pattern }}
  zero_hallucination: true
  provenance_tracking: true

regulatory:
  frameworks:
{% for framework in cookiecutter.frameworks %}
    - {{ framework }}
{% endfor %}
  standards:
{% for standard in cookiecutter.standards %}
    - {{ standard }}
{% endfor %}

validation:
  input_validation: true
  output_validation: true
  schema_version: "1.0"

dependencies:
{% for dep in cookiecutter.dependencies %}
  - {{ dep }}
{% endfor %}

certification:
  required_dimensions:
    - D01  # Determinism
    - D02  # Provenance
    - D03  # Zero-Hallucination
    - D04  # Accuracy
    - D07  # Regulatory Compliance
  target_level: SILVER
